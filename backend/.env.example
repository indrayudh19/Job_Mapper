# Environment variables for India Job Map Backend
# Copy this file to .env and fill in your API keys

# OpenAI API Key (required for extraction and embeddings)
OPENAI_API_KEY=your-openai-api-key-here

# Pinecone API Key (required for vector storage)
PINECONE_API_KEY=your-pinecone-api-key-here
PINECONE_ENVIRONMENT=us-east-1

# Optional: Use FAISS instead of Pinecone (set to "true" to use local FAISS)
USE_LOCAL_VECTORDB=false
